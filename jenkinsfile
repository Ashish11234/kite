pipeline {
    agent any

    stages {
        stage('Git') {
            steps {
                git branch: 'feature', url: 'https://github.com/Ashish11234/kite.git'
            }
        }
        stage("shell"){
            steps{
                sh '''chmod 777 file1.sh
                ./file1.sh
                chmod 777 file2.sh
               ./file2.sh'''
            }
        }
    }
}
